#генерируем список их 10 случайных чисел от 0 до 100. 
import random
a_list = [random.randint(0, 100) for x in range(1, 11)]
print(a_list)
a_list = [50, 4,7,16]

N=len(a_list)

for i in range(N-1):
    for j in range(N-i-1):
        if a_list[j] > a_list[j+1]:
            a_list[j], a_list[j+1] = a_list[j+1], a_list[j]
 
print(a_list)
